{
  "sourceMaps": true,
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "h",
        "pragmaFrag": "Fragment"
      }
    ]
  ]
}